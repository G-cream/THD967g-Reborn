    local group g
    local unit u
    local unit v
    local integer level= LoadInteger(udg_ht, task, 0)
    local integer i= LoadInteger(udg_ht, task, 1)
    //========
    if i == 0 and (not IsUnitDead((caster))) then // INLINED!!
        call SaveInteger(udg_ht, task, 1, i + 1)
