function Trig_YukkuriTransport_Conditions takes nothing returns boolean
    if GetSpellAbilityId() != 'A0ZF' then
        return false
    endif
    return true
endfunction
function AngleReset takes real angle returns real
    if angle >= 180.0 then
        return ( angle - 360.0 )
    endif
    return angle
endfunction
function Trig_YukkuriTransport_Actions takes nothing returns nothing
    local unit Manjyu= GetTriggerUnit()
    local player p= GetOwningPlayer(Manjyu)
    local unit Shyoujo= udg_PlayerHeroes[GetPlayerId(p)]
    local real angle
    local item si
    local item mi
    local integer i
    local integer j
    
    set angle=AngleBetweenUnits(Shyoujo , Manjyu)
    call SetUnitX(Manjyu, GetUnitX(Shyoujo) + 64.0 * CosBJ(angle))
    call SetUnitY(Manjyu, GetUnitY(Shyoujo) + 64.0 * SinBJ(angle))
    call SetUnitFacing(Manjyu, AngleReset(angle + 180))
    
    set i=0
    set j=0
    loop
        set si=UnitItemInSlot(Shyoujo, i)
        if si == null then
            set j=0
            loop
            set mi=UnitItemInSlot(Manjyu, j)
            if mi != null then
                call UnitAddItem(Shyoujo, mi)
                call UnitRemoveItem(Manjyu, mi)
                call TriggerSleepAction(0.01)
                set j=5
            endif
            exitwhen j >= 5
            set j=j + 1
            endloop
        endif
        exitwhen i >= 5
        set i=i + 1
    endloop
    
    set Manjyu=null
    set p=null
    set Shyoujo=null
    set mi=null
    set si=null
endfunction
