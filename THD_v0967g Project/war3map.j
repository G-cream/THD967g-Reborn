function Trig_Shikieiki01_Active_Conditions takes nothing returns boolean
    local unit target
    local boolean k
    if GetKillingUnit() == null then
        return false
    endif
    if GetUnitAbilityLevel(GetTriggerUnit(), 'Aloc') > 0 then
        return false
    endif
    if IsUnitIllusion(GetTriggerUnit()) then
        return false
