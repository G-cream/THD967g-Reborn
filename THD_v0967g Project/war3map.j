    local real tx= GetUnitX(target)
    local real ty= GetUnitY(target)
    local unit e1= CreateUnit(GetOwningPlayer(caster), 'e01R', tx, ty, 0)
    local unit v
    local unit w
    local group g= CreateGroup()
