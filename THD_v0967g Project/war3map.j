    if IsUnitVisible(target, GetLocalPlayer()) then
      call SetTextTagColor(e, 0, 220, 0, 240)
    else
      call SetTextTagColor(e, 0, 220, 0, 0)
    endif
    call SetTextTagVelocityBJ(e, 150, 90)
