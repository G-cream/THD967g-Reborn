    call SaveReal(udg_ht, task, 0, a)
    call SaveReal(udg_ht, task, 1, d)
    
    set s=2.0 + 0.3 * ( level - 1 )
    set r=85.0 + 5.0 * level
    
    set a=a + 120.0
    set px=ox + d * CosBJ(a)
    set py=oy + d * SinBJ(a)
