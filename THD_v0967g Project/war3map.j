    call SaveInteger(udg_ht, task, 2, id1)
    call TimerStart(t, 0.00, false, function Trig_Statis_Damage_Another)
    
    set udg_PlayerDamage[id1 * 16 + id2]=udg_PlayerDamage[id1 * 16 + id2] + amount
    set udg_PlayerDamage[id1 * 16 + 14]=udg_PlayerDamage[id1 * 16 + 14] + amount
