function Trig_Set_GIB_Clock_Actions takes nothing returns nothing
    local string str
    local integer h= udg_GameTime / 3600
    local integer m= udg_GameTime / 60
    local integer s= udg_GameTime
    local string a= ""
    //========
    set m=m - ( m / 60 ) * 60
    set s=s - ( s / 60 ) * 60
    if h > 0 then
        set a=a + I2S(h)
        set a=a + ":"
        if m >= 10 then
            set a=a + I2S(m)
        else
            set a=a + "0" + I2S(m)
        endif
        set a=a + ":"
        if s >= 10 then
            set a=a + I2S(s)
        else
            set a=a + "0" + I2S(s)
        endif
    else
        set a=a + I2S(m)
        set a=a + ":"
        if s >= 10 then
            set a=a + I2S(s)
        else
            set a=a + "0" + I2S(s)
        endif
    endif
    //========
    set str="|cffffcc00|r / "
    set str=str + "|cffffcc00|r  "
    set str=str + a
    call MultiboardSetTitleText(udg_GIB, str)
    
    //========
    set str=udg_PlayerColors[5] + ":       " + "|r" + I2S(udg_ScoreSpirit[4])
    call MultiboardSetItemValue(MultiboardGetItem(udg_GIB, udg_GIB_PlayerRow[0], 1), str)
    
    set str=udg_PlayerColors[10] + ":       " + "|r" + I2S(udg_ScoreSpirit[10])
    call MultiboardSetItemValue(MultiboardGetItem(udg_GIB, udg_GIB_PlayerRow[0], 2), str)
endfunction
