function Trig_Statis_Damage_Conditions takes nothing returns boolean
    if GetEventDamage() <= 1.00 then
        return false
    endif
    if IsUnitIllusion(GetTriggerUnit()) then
        return false
    endif
    if GetOwningPlayer(GetEventDamageSource()) == udg_PlayerA[0] or GetOwningPlayer(GetEventDamageSource()) == udg_PlayerB[0] then
        return false
    endif
    if ( not IsUnitEnemy(GetTriggerUnit(), GetOwningPlayer(GetEventDamageSource())) ) then
        return false
    endif
    
    return IsUnitType(GetTriggerUnit(), UNIT_TYPE_HERO) and IsPlayerInForce(GetOwningPlayer(GetTriggerUnit()), udg_OnlinePlayers) and IsPlayerInForce(GetOwningPlayer(GetEventDamageSource()), udg_OnlinePlayers)
endfunction
function Trig_Statis_Damage_Another takes nothing returns nothing
    local timer t= GetExpiredTimer()
    local integer task= GetHandleId(t)
    local unit shouhairen= LoadUnitHandle(udg_ht, task, 0)
    local real lifebefore= LoadReal(udg_ht, task, 1)
    local integer id1= LoadInteger(udg_ht, task, 2)
    local integer id2= GetPlayerId(GetOwningPlayer(shouhairen))
    local real via= lifebefore - GetUnitState(shouhairen, UNIT_STATE_LIFE)
    
    set udg_PlayerDamageAnother[id1 * 16 + id2]=udg_PlayerDamageAnother[id1 * 16 + id2] + via
    set udg_PlayerDamageAnother[id1 * 16 + 14]=udg_PlayerDamageAnother[id1 * 16 + 14] + via
    set udg_PlayerDamageAnother[id2 * 16 + 15]=udg_PlayerDamageAnother[id2 * 16 + 15] + via
    
    call PauseTimer(t)
    call DestroyTimer(t)
    call FlushChildHashtable(udg_ht, task)
    set t=null
    set shouhairen=null
endfunction
function Trig_Statis_Damage_Actions takes nothing returns nothing
    local timer t= CreateTimer()
    local integer task= GetHandleId(t)
    local unit shouhairen= GetTriggerUnit()
    local unit huaidan= GetEventDamageSource()
    local integer id1= GetPlayerId(GetOwningPlayer(huaidan))
    local integer id2= GetPlayerId(GetOwningPlayer(shouhairen))
    local real amount= GetEventDamage()
    
    call SaveUnitHandle(udg_ht, task, 0, shouhairen)
