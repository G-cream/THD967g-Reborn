    call SaveUnitHandle(udg_ht, task, 1, u)
    call SaveGroupHandle(udg_ht, task, 2, CreateGroup())
    call SaveGroupHandle(udg_ht, task, 3, CreateGroup())
    call SaveInteger(udg_ht, task, 1, i + 1)
