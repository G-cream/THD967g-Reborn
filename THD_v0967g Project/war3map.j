        call IssueImmediateOrder(caster, "stop")
        call SetUnitPathing(caster, true)
        call KillUnit(u)
        //call DestroyEffect(e)
        call DestroyTimer(t)
        call DestroyTrigger(trg)
        call FlushChildHashtable(udg_Hashtable, task)
