    //========
    if IsUnitAliveBJ(u) then
        set h=h + v
        call SetUnitFlyHeight(u, h, 2000)
        if h < 5.0 and v < 0 then
            set v=- 1 * f * v
        endif
        call SaveReal(udg_ht, task, 0, v + g)
