    local real oy= GetUnitY(caster)
    local real oz
    local real px
    local real py
    local real pz
    local real a
    local real d
    local integer i= LoadInteger(udg_ht, task, 1)
    //========
    if (not IsUnitDead((caster))) and (not IsUnitDead((u))) then // INLINED!!
        if i >= 0 then
            if i <= 12 then
                call SaveInteger(udg_ht, task, 1, i + 1)
            else
                call SaveInteger(udg_ht, task, 1, 0)
            endif
        endif
        if not IsUnitInRange(caster, u, 1000.0) then
            set px=GetUnitX(u)
            set py=GetUnitY(u)
            set a=Atan2(py - oy, px - ox)
            set d=GetRandomReal(600.0, 800.0)
