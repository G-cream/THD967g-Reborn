    endif
    call SetUnitAbilityLevel(u1, 'A0JE', level)
    call SetUnitAbilityLevel(u2, 'A0JE', level)
    call SetUnitAbilityLevel(u3, 'A0JE', level)
    call IssuePointOrder(u1, "stampede", tx + 100.0 * Cos(a), ty + 100.0 * Sin(a))
    call IssuePointOrder(u2, "stampede", tx + 100.0 * Cos(a + 2.0 / 3.0 * 3.1415926535), ty + 100.0 * Sin(a + 2.0 / 3.0 * 3.1415926535))
    call IssuePointOrder(u3, "stampede", tx + 100.0 * Cos(a + 4.0 / 3.0 * 3.1415926535), ty + 100.0 * Sin(a + 4.0 / 3.0 * 3.1415926535))
