    endif
    if IsUnitType(GetTriggerUnit(), UNIT_TYPE_FLYING) then
        return false
    endif
    if IsUnitType(GetTriggerUnit(), UNIT_TYPE_STRUCTURE) then
        return false
    endif
    if GetUnitAbilityLevel(GetTriggerUnit(), 'Aloc') != 0 then
