    call UnitApplyTimedLife(u, 'BTLF', 0.5)
    call UnitAddAbility(u, 'A0E2')
    set i=0
    loop
        exitwhen i >= 24
        set px=ox + 100.0 * CosBJ(15.0 * i)
        set py=oy + 100.0 * SinBJ(15.0 * i)
        call IssuePointOrder(u, "breathoffrost", px, py)
        set i=i + 1
    endloop
    call CastSpell(caster , "")
    //========
    set caster=null
    set t=null
    set u=null
