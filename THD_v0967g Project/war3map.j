    local integer ID
    local integer m= (udg_HC_Database_01[2000]) // INLINED!!
    set SN=0
    loop
        exitwhen SN >= m
        set ID=HC_DataBaseRead(DB , SN , 1)
        if ID == TargetID then
            return SN
        endif
        set SN=SN + 1
    endloop
    return - 1
endfunction
