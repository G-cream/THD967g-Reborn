    if GetPlayerSlotState(Player(8)) == PLAYER_SLOT_STATE_PLAYING then
        set p=p + 1
    endif
    if GetPlayerSlotState(Player(9)) == PLAYER_SLOT_STATE_PLAYING then
        set p=p + 1
    endif
