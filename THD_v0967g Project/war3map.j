            set udg_CSS_register[6]=1
            call CSS_FillRandomCharacters()
        else
            set udg_CSS_register[6]=0
            call TriggerExecute(gg_trg_CSS_AI)
        endif
    endif
    
    if (( udg_GameMode / 10 ) == 3) then // INLINED!!
        call Trig_CSS_Random_Ban_Actions()
    endif
    //========
    
    set t=null
    set w=null
