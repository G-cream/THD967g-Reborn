function PhysicalDamageCritical takes unit caster,unit u,real damage returns nothing
    local real tredam= damage
    local real critp= 0.00
    local real critx= 1.00
    local integer i
    //==================================
    //600% - 'A06R'-Lv3
    set critx=6.00
    set critp=0.00
    if GetUnitAbilityLevel(caster, 'A06R') == 3 then
        set critp=100
    endif
    if GetRandomInt(1, 100) <= critp then
        set tredam=tredam * critx
        call PhysicalDamageCriticalInRed(u , tredam)
        call UnitDamageTarget(caster, u, tredam, true, false, ATTACK_TYPE_HERO, DAMAGE_TYPE_NORMAL, WEAPON_TYPE_WHOKNOWS)
        return
    endif
    //==================================
    //450% - 'A06R'-Lv2
    set critx=4.50
    set critp=0.00
    if GetUnitAbilityLevel(caster, 'A06R') == 2 then
        set critp=100
    endif
    if GetRandomInt(1, 100) <= critp then
        set tredam=tredam * critx
        call PhysicalDamageCriticalInRed(u , tredam)
        call UnitDamageTarget(caster, u, tredam, true, false, ATTACK_TYPE_HERO, DAMAGE_TYPE_NORMAL, WEAPON_TYPE_WHOKNOWS)
        return
    endif
    //==================================
    //300% - 'I008' , 'A06R'-Lv1
    set critx=3.00
    set critp=0.00
    set i=0
    loop
        set i=i + 1
        if GetItemTypeId(UnitItemInSlotBJ(caster, i)) == 'I008' then
