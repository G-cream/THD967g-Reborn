function Trig_Eirin03_Main takes nothing returns nothing
    local timer t= GetExpiredTimer()
    local integer task= GetHandleId(t)
    local unit target= LoadUnitHandle(udg_ht, task, 0)
    local integer level= LoadInteger(udg_ht, task, 1)
    local integer i= LoadInteger(udg_ht, task, 2)
    if GetUnitAbilityLevel(target, 'B01P') == 0 then
        call UnitAddAbility(target, 'A0YZ')
        call SetUnitAbilityLevel(target, 'A0YZ', level)
        call UnitRemoveAbility(target, 'A0Z0')
    else
        call UnitRemoveAbility(target, 'A0YZ')
    endif
    set i=i - 1
    if i == 0 then
        if GetUnitAbilityLevel(target, 'A0N0') == 0 then
            if GetUnitAbilityLevel(target, 'A0YY') - level != 0 and GetUnitAbilityLevel(target, 'A0YY') >= 1 then
                call SetUnitAbilityLevel(target, 'A0YY', GetUnitAbilityLevel(target, 'A0YY') - level)
            elseif GetUnitAbilityLevel(target, 'A0YY') >= 1 then
                call UnitRemoveAbility(target, 'A0YY')
            endif
        endif
        if GetUnitAbilityLevel(target, 'A0Z0') - level != 0 then
            call SetUnitAbilityLevel(target, 'A0Z0', GetUnitAbilityLevel(target, 'A0Z0') - level)
        else
            call UnitRemoveAbility(target, 'A0Z0')
        endif
        call UnitRemoveAbility(target, 'A0YZ')
        call DestroyTimer(t)
        call FlushChildHashtable(udg_ht, task)
    endif
    call SaveInteger(udg_ht, task, 2, i)
    set t=null
    set target=null
