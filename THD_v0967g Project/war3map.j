            call AdjustPlayerStateSimpleBJ(GetOwningPlayer(GetSoldUnit()), PLAYER_STATE_RESOURCE_GOLD, costA) //
            call THD_SyncSpirit()
        else
            set h=GetPlayerCharacter(who)
