    call ResumeMusic()
    // ========
    call YDWEPolledWaitNull(20.00)
    call TriggerExecute(gg_trg_Announce)
    call YDWEPolledWaitNull(30.00)
    // 
    call EnableTrigger(gg_trg_Spawn_A)
