    local unit u= LoadUnitHandle(udg_Hashtable, task, 1)
    local integer i= LoadInteger(udg_Hashtable, task, 2)
    local location loc= GetUnitLoc(u)
    local group loli= YDWEGetUnitsInRangeOfLocMatchingNull(350 , loc , Condition(function Trig_XiongGui_loli_Conditions))
