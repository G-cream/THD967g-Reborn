    //call SaveTimerHandle(udg_Hashtable, GetHandleId(tr), 2, t2)
    //call SaveTriggerHandle(udg_Hashtable, tid, 4, tr)
    
    //set tr = null
    set caster=null
    set t=null
    set t2=null
endfunction
