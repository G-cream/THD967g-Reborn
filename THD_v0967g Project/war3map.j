    call TriggerRegisterPlayerStateEvent(gg_trg_Statis_GPM, Player(7), PLAYER_STATE_RESOURCE_GOLD, GREATER_THAN_OR_EQUAL, 0.00)
    call TriggerRegisterPlayerStateEvent(gg_trg_Statis_GPM, Player(8), PLAYER_STATE_RESOURCE_GOLD, GREATER_THAN_OR_EQUAL, 0.00)
    call TriggerRegisterPlayerStateEvent(gg_trg_Statis_GPM, Player(9), PLAYER_STATE_RESOURCE_GOLD, GREATER_THAN_OR_EQUAL, 0.00)
    call TriggerRegisterAnyUnitEventBJ(gg_trg_Statis_GPMRevise, EVENT_PLAYER_UNIT_PAWN_ITEM)
    call RegisterAnyUnitDamage(gg_trg_Statis_Damage)
    // ========
    call ForForce(udg_OnlinePlayers, function Trig_Game_StartFunc047A)
    call TriggerRegisterPlayerUnitEventSimple(gg_trg_Point_Creat, udg_PlayerA[0], EVENT_PLAYER_UNIT_DEATH)
    call TriggerRegisterPlayerUnitEventSimple(gg_trg_Point_Creat, udg_PlayerB[0], EVENT_PLAYER_UNIT_DEATH)
