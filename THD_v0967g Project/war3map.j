//===========================================================================
// Trigger: Statis Kill
//
// PlayerKill0-11id+16+16
// GIB
//===========================================================================
//TESH.scrollpos=14
//TESH.alwaysfold=0
function Trig_Statis_Kill_Conditions takes nothing returns boolean
    if IsUnitType(GetTriggerUnit(), UNIT_TYPE_TAUREN) then
        return false
    endif
    if GetUnitTypeId(GetTriggerUnit()) == 'o000' then
        return false
    endif
    if GetUnitTypeId(GetTriggerUnit()) == 'n001' then
        return false
    endif
    
    return IsUnitSpawn(GetTriggerUnit()) or GetOwningPlayer(GetTriggerUnit()) == Player(PLAYER_NEUTRAL_AGGRESSIVE)
endfunction
function Trig_Statis_Kill_Actions takes nothing returns nothing
    local unit dead= GetTriggerUnit()
    local unit kira= GetKillingUnit()
    local integer id= GetPlayerId(GetOwningPlayer(GetKillingUnit()))
    local string text= ""
    local multiboarditem field
    
    if IsUnitSpawn(dead) then
        if IsUnitAlly(dead, Player(id)) then
            set udg_PlayerKill[id + 16]=udg_PlayerKill[id + 16] + 1
        elseif IsUnitEnemy(dead, Player(id)) then
            set udg_PlayerKill[id]=udg_PlayerKill[id] + 1
        endif
    endif
    
    if GetOwningPlayer(dead) == Player(PLAYER_NEUTRAL_AGGRESSIVE) then
        if IsUnitEnemy(dead, Player(id)) then
            set udg_PlayerKill[id + 32]=udg_PlayerKill[id + 32] + 1
        endif
    endif
    
    set text=I2S(udg_PlayerKill[id]) + "/"
    set text=text + I2S(udg_PlayerKill[id + 16]) + "/"
    set text=text + I2S(udg_PlayerKill[id + 32])
    
    set field=MultiboardGetItem(udg_GIB, udg_GIB_PlayerRow[0] + 1, 1)
    
    if ( GetLocalPlayer() == Player(id) ) then
        call MultiboardSetItemValue(field, text)
    endif
    
    set field=null
    set dead=null
    set kira=null
    set text=null
endfunction
