function Trig_Lonesome_Main takes nothing returns nothing
    local timer t= GetExpiredTimer()
    local integer task= GetHandleId(t)
    local unit caster= LoadUnitHandle(udg_ht, task, 0)
    local real oldhp= LoadReal(udg_ht, task, 1)
    local integer i= LoadInteger(udg_ht, task, 2)
    local integer k= LoadInteger(udg_ht, task, 3)
    local real newhp
    local real maxhp
    local real nowhp
    set i=i + 1
    set nowhp=GetUnitState(caster, UNIT_STATE_LIFE)
    if i >= 50 then
        set i=i - 50
        set maxhp=GetUnitState(caster, UNIT_STATE_MAX_LIFE)
        call SetUnitState(caster, UNIT_STATE_LIFE, nowhp + 0.015 * ( maxhp - nowhp ))
        set nowhp=GetUnitState(caster, UNIT_STATE_LIFE)
    endif
    if nowhp > oldhp then
        set newhp=nowhp + ( nowhp - oldhp ) * 0.30
        call SetUnitState(caster, UNIT_STATE_LIFE, newhp)
    endif
    call SaveReal(udg_ht, task, 1, GetUnitState(caster, UNIT_STATE_LIFE))
    call SaveInteger(udg_ht, task, 2, i)
    if udg_SK_Lonesome[k] == 0 then
        call PauseTimer(t)
        call DestroyTimer(t)
        call FlushChildHashtable(udg_ht, task)
    endif
    set t=null
    set caster=null
endfunction
function Trig_Lonesome_Actions takes nothing returns nothing
    local unit caster= GetTriggerUnit()
    local integer k= GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))
    local timer t
    local integer task
    if udg_SK_Lonesome[k] + 1 == 1 then
        set t=CreateTimer()
        set task=GetHandleId(t)
        call SaveUnitHandle(udg_ht, task, 0, caster)
        call SaveReal(udg_ht, task, 1, GetUnitState(caster, UNIT_STATE_LIFE))
        call SaveInteger(udg_ht, task, 2, 0)
        call SaveInteger(udg_ht, task, 3, k)
        call TimerStart(t, 0.02, true, function Trig_Lonesome_Main)
    endif
    set udg_SK_Lonesome[k]=udg_SK_Lonesome[k] + 1
    set caster=null
    set t=null
endfunction
