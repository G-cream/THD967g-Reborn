    if IsUnitType(GetTriggerUnit(), UNIT_TYPE_HERO) == false then
        return false
    endif
    if IsUnitIllusion(GetTriggerUnit()) then
        return false
    endif
    if GetUnitAbilityLevel(GetAttacker(), 'A0SY') == 0 then
