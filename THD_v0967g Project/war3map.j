    local real ox= GetUnitX(h)
    local real oy= GetUnitY(h)
    local real dx= GetCameraTargetPositionX() - ox
    local real dy= GetCameraTargetPositionY() - oy
    local real d= SquareRoot(dx * dx + dy * dy)
    local integer l= Trig_Yukari04_New_Distance(d)
    local integer k
    local integer j
    
    set k='A0IA'
    set j=0
    loop
        exitwhen k > 'A0IG'
        if GetUnitAbilityLevel(h, k) == 1 then
            set j=100 + 15 * l
        elseif GetUnitAbilityLevel(h, k) == 2 then
            set j=83 + 12 * l
        elseif GetUnitAbilityLevel(h, k) == 3 then
            set j=66 + 9 * l
        endif
        set k=k + 1
    endloop
    if j != 0 then
        call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "" + R2S(d) + "" + I2S(j) + "")
    endif
    call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "" + R2S(udg_SK_Yukari_Count))
