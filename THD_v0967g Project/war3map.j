    set target=GetPlayerCharacter(GetOwningPlayer(GetKillingUnit()))
    if target == null then
        return false
    endif
    set k=(not IsUnitDead((target))) and GetUnitAbilityLevel(target, 'B02A') > 0 // INLINED!!
