function Trig_Yukari02_Dump_Main takes nothing returns nothing
    local timer t= GetExpiredTimer()
    local integer task= GetHandleId(t)
    local unit caster= LoadUnitHandle(udg_ht, task, 0)
    local unit u= LoadUnitHandle(udg_ht, task, 1)
    local unit v
    local player w= GetOwningPlayer(caster)
    local real r
    local real x
    local real y
    local effect e= LoadEffectHandle(udg_ht, task, 2)
    local integer i= LoadInteger(udg_ht, task, 1)
    local integer level
    local integer abilevel= GetUnitAbilityLevel(caster, 'A0GK')
    local real damage
    local group g
    local boolexpr iff
    //========
    if i > 0 then
        if IsUnitDead(u) then
            call DestroyEffect(e)
            call SaveEffectHandle(udg_ht, task, 2, null)
        endif
        call SetUnitFlyHeight(u, 80.0 * i, 1600.00)
        call SaveInteger(udg_ht, task, 1, i - 1)
    else
        if e != null then
            call DestroyEffect(e)
        endif
        if true then
            call PauseUnit(u, false)
            call SetUnitFlyHeight(u, 0.0, 1600.0)
            call THD_ResetSpawnUnitOrder(u)
            set level=abilevel
            set r=210.0
            set x=GetUnitX(u)
            set y=GetUnitY(u)
            set damage=90 + abilevel * 30.0
            call DestroyEffect(AddSpecialEffect("Abilities\\Spells\\Orc\\WarStomp\\WarStompCaster.mdl", x, y))
            call UnitDamageTarget(caster, u, 100 + 0.25 * GetUnitState(u, UNIT_STATE_MAX_LIFE), false, false, ATTACK_TYPE_HERO, DAMAGE_TYPE_MAGIC, null)
            set u=CreateUnit(GetOwningPlayer(caster), ('o000'), x, y, 0.0) // INLINED!!
            call UnitAddAbility(u, 'A0GJ')
            call SetUnitAbilityLevel(u, 'A0GJ', level)
            set g=CreateGroup()
            set iff=(udg_FLIFF[GetPlayerId((w))]) // INLINED!!
            call GroupEnumUnitsInRange(g, x, y, r, iff)
            loop
                set v=FirstOfGroup(g)
                exitwhen v == null
                call GroupRemoveUnit(g, v)
                if IsUnitAliveBJ(v) and IsUnitEnemy(v, w) and (GetUnitAbilityLevel((v), 'A0IL') > 0) == false and IsUnitType(v, UNIT_TYPE_MECHANICAL) == false then // INLINED!!
                      call UnitMagicDamageTarget(caster , v , damage)
                      call UnitStunTarget(caster , v , 0.5 + 0.25 * level , 0 , 0)
                endif
            endloop
            call DestroyGroup(g)
        endif
        call DestroyTimer(t)
        call FlushChildHashtable(udg_ht, task)
    endif
    //========
    set t=null
    set caster=null
    set e=null
    set u=null
    set v=null
    set w=null
    set g=null
    set iff=null
endfunction
