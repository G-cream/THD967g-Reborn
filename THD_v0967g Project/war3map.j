        set s=level * 50 + 50
        call SetUnitState(target, UNIT_STATE_LIFE, GetUnitState(target, UNIT_STATE_LIFE) + s)
    else
        //set w = CreateUnit(GetOwningPlayer(caster),'n001',GetUnitX(target),GetUnitY(target),0)
        //call UnitAddAbility(w,'A0BA')
