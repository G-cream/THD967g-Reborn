    local unit caster= LoadUnitHandle(udg_ht, task, 0)
    local group g
    local unit v
    local real ox= GetUnitX(caster)
    local real oy= GetUnitY(caster)
    local real damage
    set g=CreateGroup()
    call GroupEnumUnitsInRange(g, ox, oy, 99999, Filter(function Trig_MediEx_Target))
