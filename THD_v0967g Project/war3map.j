        call UnitAddAbility(caster, 'A0E1')
    endif
    call SetUnitFlag(caster , (1) , true) // INLINED!!
    call TimerStart(t, 0.10, true, function Trig_Youmu04_Main)
    //========
    set caster=null
    set target=null
    set e[0]=null
