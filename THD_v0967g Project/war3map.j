//
function CSS_RequestCharacter takes player who,integer index returns boolean
    local integer i
    local integer j
    local integer node= udg_CSS_register[7]
    
    if index <= 0 or index == 100 or index >= 200 then
        return false
    endif
     //RandomCloth
    if index == 116 and GetRandomInt(1, 100) < 35 then
        set index=202
    elseif index == 14 and GetRandomInt(1, 100) < 30 then
        set index=203
    endif
    
    //
    if udg_HeroBan[index] then
        return false
    endif
    
    // 
    set i=11
    loop
        exitwhen udg_PlayerHeroList[i] == index
        if index == 116 or index == 202 then //RandomCloth - Mystia
exitwhen udg_PlayerHeroList[i] == 116 or udg_PlayerHeroList[i] == 202
        elseif index == 14 or index == 203 then //RandomCloth - 16
exitwhen udg_PlayerHeroList[i] == 14 or udg_PlayerHeroList[i] == 203
        endif
        set i=i - 1
        exitwhen i < 0
    endloop
    if not ( i < 0 ) then
        return false
    endif
    
    // 
    if (( udg_GameMode / 10 ) == 2) then // INLINED!!
        set udg_PlayerHeroList[GetPlayerId(who)]=index
        return true
    elseif (( udg_GameMode / 10 ) == 3) then // INLINED!!
        set udg_PlayerHeroList[GetPlayerId(who)]=index
        return true
    endif
    
    //
    if (( udg_GameMode - udg_GameMode / 10 * 10 ) == 5) and node <= 6 then // INLINED!!
        //BANBAN
        if ( index > 100 ) != IsPlayerInForce(who, udg_TeamB) then
            set udg_PlayerHeroList[GetPlayerId(who)]=index
            return true
        endif
    else
        //
        if ( index > 100 ) == IsPlayerInForce(who, udg_TeamB) then
            set udg_PlayerHeroList[GetPlayerId(who)]=index
            return true
        endif
    endif
    
    return false
endfunction
//
function CSS_RequestRandom takes player who returns integer
    local integer m
    local integer n
    local integer w
    local integer i
    local integer index
    local boolean hiddencharacters= false
    local real hiddenvira= 8
    local boolean hiddenex= false
    
    //Hidden Characters
    //Mugetsu
    //GetIn AllRandomMode with 0.1% chance
    //GetInHaving TewiYukariYukaSakuyaReimuAliceMarisa AtTheSameGame
    set w=0
    loop
        //Usual Characters====================
        //Leader - Yukari
        if udg_HeroType[udg_PlayerHeroList[w]] == 'E00F' then
            set hiddenvira=hiddenvira - 1
        endif
        //Lucky - Tewi
        if udg_HeroType[udg_PlayerHeroList[w]] == 'O00N' then
            set hiddenvira=hiddenvira - 1
        endif
        //Related Characters====================
        //Yuka
        if udg_HeroType[udg_PlayerHeroList[w]] == 'N00L' then
            set hiddenvira=hiddenvira - 1
        endif
        //Maid - Sakuya
        if udg_HeroType[udg_PlayerHeroList[w]] == 'E000' then
            set hiddenvira=hiddenvira - 1
        endif
        //Reimu
        if udg_HeroType[udg_PlayerHeroList[w]] == 'H001' then
            set hiddenvira=hiddenvira - 1
        endif
        //Alice
        if udg_HeroType[udg_PlayerHeroList[w]] == 'H01A' then
            set hiddenvira=hiddenvira - 1
        endif
        //Marisa
        if udg_HeroType[udg_PlayerHeroList[w]] == 'H000' then
            set hiddenvira=hiddenvira - 1
        endif
        //Other Factor====================
        //End Counting====================
        set w=w + 1
        exitwhen w > 12
    endloop
    if (( udg_GameMode - udg_GameMode / 10 * 10 ) == 4) then // INLINED!!
        set hiddenex=GetRandomInt(1, 2000) <= 2
    elseif hiddenvira == 1 then
        set hiddenex=true
    endif
    if hiddenex then
        set index=201
        set w=0
        loop
            exitwhen udg_PlayerHeroList[w] == index
            set w=w + 1
            exitwhen w > 12
        endloop
        set hiddencharacters=true
        set w=0
        loop
            if udg_PlayerHeroList[w] == index then
                set hiddencharacters=false
            endif
            set w=w + 1
            exitwhen w > 12
        endloop
    endif
    //Normal Characters
    if hiddencharacters == false then
        // 
        if (( udg_GameMode / 10 ) == 2) then // INLINED!!
            set w=GetRandomInt(1, udg_HeroType[0] + udg_HeroType[100])
            set i=Integer(w > udg_HeroType[0])
        else
            set i=Integer(IsPlayerInForce(who, udg_TeamB))
        endif
        //
        set n=i * 100 + 1
        set m=i * 100 + udg_HeroType[i * 100]
        set i=0
        loop
            set index=GetRandomInt(n, m)
            set w=0
            loop
                exitwhen udg_HeroBan[index]
                exitwhen udg_PlayerHeroList[w] == index
                if udg_PlayerHeroList[w] == 116 or udg_PlayerHeroList[w] == 202 then //RandomCloth - Mystia
exitwhen index == 116 or index == 202
                elseif udg_PlayerHeroList[w] == 14 or udg_PlayerHeroList[w] == 203 then //RandomCloth - 16
exitwhen index == 14 or index == 203
                endif
                set w=w + 1
                exitwhen w > 12
            endloop
            set i=i + 1
            exitwhen i >= 50
        endloop
    endif
    
    //
    if w > 11 then
        if (( udg_GameMode - udg_GameMode / 10 * 10 ) == 5) then // INLINED!!
        else
            set udg_PlayerHeroList[GetPlayerId(who)]=index
        endif
        return index
    endif
    
    return 0
endfunction
