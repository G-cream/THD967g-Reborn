function Koishi01_Resolve takes unit h,integer x returns nothing
    local integer i= 6
    loop
        exitwhen i == 0
        set i=i - 1
        call UnitRemoveAbility(h, 'A0DS' + i)
        if GetBitBoolean(x , i) then
            call UnitAddAbility(h, 'A0DS' + i)
        endif
    endloop
