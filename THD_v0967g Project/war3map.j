function Trig_LunasaDamageInRed takes unit u,real tredam returns nothing
    local texttag e= CreateTextTag()
    call SetTextTagTextBJ(e, I2S(R2I(tredam)) + "!", 12.0)
    call SetTextTagPos(e, GetUnitX(u), GetUnitY(u), 100.0)
    call SetTextTagColor(e, 220, 0, 220, 240)
    call SetTextTagVelocityBJ(e, 150, 90)
    call SetTextTagPermanent(e, false)
    call SetTextTagLifespan(e, 1.67)
    set e=null
endfunction
function Trig_LunasaDamage takes unit caster,unit u,real damage returns nothing
    local integer critlevel= GetUnitAbilityLevel(caster, 'A0OG')
    local integer bufflevel= udg_SK_Lunasa02_Buff
    local real tredam
    local real h0= GetUnitState(caster, UNIT_STATE_LIFE)
    local real h1= GetUnitState(caster, UNIT_STATE_MAX_LIFE)
    local real c= 26 - critlevel * 4
    local real rmin= ( 1 - ( h0 / h1 ) ) * ( 100 / c ) * 10
    if YDWEUnitHasItemOfTypeBJNull(GetPlayerCharacter(GetOwningPlayer(caster)) , 'I064') then //Hakkero
set tredam=damage * ( 1 + bufflevel * 0.003 + 0.30 )
    else
        set tredam=damage * ( 1 + bufflevel * 0.003 )
    endif
    call DestroyEffect(AddSpecialEffect("Abilities\\Weapons\\AvengerMissile\\AvengerMissile.mdl", GetUnitX(u), GetUnitY(u)))
    if critlevel > 0 and GetRandomInt(0, 100) < rmin then
        set tredam=tredam * 2.0
        call Trig_LunasaDamageInRed(u , tredam)
    endif
    call UnitDamageTarget(caster, u, tredam, false, true, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
endfunction
