        if IsUnitType(v, UNIT_TYPE_DEAD) == false and IsUnitType(v, UNIT_TYPE_STRUCTURE) == false and target != v then
            //set w = CreateUnit(GetOwningPlayer(caster),'n001',tx,ty,0)
