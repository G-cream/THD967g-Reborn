                call SaveUnitHandle(udg_ht, task, 0, v)
                call TimerStart(t, 6.00, false, function Trig_BoneClarinet_Clear)
            endif
        endif
    endloop
    call DestroyGroup(g)
    set caster=null
    set g=null
    set v=null
    set t=null
