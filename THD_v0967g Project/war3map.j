    local integer task
    if GetTriggerEventId() == EVENT_PLAYER_UNIT_DEATH then
        if GetUnitTypeId(GetTriggerUnit()) == 'h00O' and GetKillingUnit() != null then
            set task=GetHandleId(GetTriggerUnit())
