    if mantou == null then
        return
    endif
    if GetUnitState(mantou, UNIT_STATE_LIFE) <= 0 then
        return
    endif
    
    if ( GetOwningPlayer(mantou) == Player(id) ) then
        loop
            if ( UnitItemInSlot(mantou, i) != null ) then
                set plus=plus + 10 * GetItemLevel(UnitItemInSlot(mantou, i))
            endif
            set i=i + 1
            exitwhen i > 6
        endloop
    endif
    call SaveInteger(udg_Hashtable, StringHash("whocares"), 1, plus)
    
    set mantou=null
endfunction
function bin takes nothing returns boolean
    return IsItemVisible(GetFilterItem())
endfunction
function Trash takes nothing returns nothing
    local item trash= GetEnumItem()
    local integer plus= 0
    local integer id= LoadInteger(udg_Hashtable, StringHash("whocares"), 2)
    
    if GetEnumItem() == null then
        return
    endif
    
    if ( THD_GetItemOwner(trash) == Player(id) ) then
        set plus=plus + 10 * GetItemLevel(trash)
    endif
    call SaveInteger(udg_Hashtable, StringHash("whocares"), 3, plus)
    
    set trash=null
endfunction
function GetPlayerNetWorth takes player ply returns integer
    local unit millionaire
    local integer amount
    local integer i
    local integer id
    local group mantou= CreateGroup()
    
    set millionaire=GetPlayerCharacter(ply)
    set amount=0
    set i=0
    set id=GetPlayerId(ply)
    
    set amount=GetPlayerState(ply, PLAYER_STATE_RESOURCE_GOLD)
    
    loop
        if ( UnitItemInSlot(millionaire, i) != null ) then
            set amount=amount + 10 * GetItemLevel(UnitItemInSlot(millionaire, i))
        endif
        set i=i + 1
        exitwhen i > 6
    endloop
    
    call SaveInteger(udg_Hashtable, StringHash("whocares"), 1, 0)
    call SaveInteger(udg_Hashtable, StringHash("whocares"), 2, id)
    call GroupEnumUnitsInRect(mantou, bj_mapInitialPlayableArea, Filter(function OnlyMantou))
    call ForGroup(mantou, function mantouPower)
    set amount=amount + LoadInteger(udg_Hashtable, StringHash("whocares"), 1)
    
    call SaveInteger(udg_Hashtable, StringHash("whocares"), 3, 0)
    call EnumItemsInRect(bj_mapInitialPlayableArea, Filter(function bin), function Trash)
    set amount=amount + LoadInteger(udg_Hashtable, StringHash("whocares"), 3)
    call FlushChildHashtable(udg_Hashtable, StringHash("whocares"))
    
    set millionaire=null
    call DestroyGroup(mantou)
    set mantou=null
    
    return amount
endfunction
