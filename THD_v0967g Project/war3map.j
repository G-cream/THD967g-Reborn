            call DestroyEffect(AddSpecialEffectTarget("Abilities\\Spells\\Human\\Resurrect\\ResurrectTarget.mdl", caster, "origin"))
        endif
        set trg=GetTriggeringTrigger()
        set t=LoadTimerHandle(udg_ht, GetHandleId(trg), 0)
    else
