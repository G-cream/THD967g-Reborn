    if SquareRoot(( GetUnitX(caster) - GetUnitX(u) ) * ( GetUnitX(caster) - GetUnitX(u) ) + ( GetUnitY(caster) - GetUnitY(u) ) * ( GetUnitY(caster) - GetUnitY(u) )) > 400.00 + level * 200 then
        set endkill=true
    endif
    if SquareRoot(( GetUnitX(target) - GetUnitX(u) ) * ( GetUnitX(target) - GetUnitX(u) ) + ( GetUnitY(target) - GetUnitY(u) ) * ( GetUnitY(target) - GetUnitY(u) )) > 400.00 + level * 200 then
        set endkill=true
    endif
    if endkill == false then
        set ltx1=GetUnitX(u)
        set lty1=GetUnitY(u)
        set ltz1=GetPositionZ(ltx1 , lty1) + 80.0
        set ltx2=GetUnitX(target)
        set lty2=GetUnitY(target)
