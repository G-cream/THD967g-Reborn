    if i > 0 and udg_SK_Letty02_Hit < udg_SK_Letty02_Max then
        call SetUnitFacing(u, bj_RADTODEG * a + 60.00 * Sin(k * 3.141592654 / 25.00))
        set px=ox + d * CosBJ(GetUnitFacing(u))
        set py=oy + d * SinBJ(GetUnitFacing(u))
