                            call KillUnit(CreateUnit(Player(PLAYER_NEUTRAL_PASSIVE), 'e014', GetUnitX(v), GetUnitY(v), 22.5))
                            call KillUnit(CreateUnit(Player(PLAYER_NEUTRAL_PASSIVE), 'e014', GetUnitX(v), GetUnitY(v), 22.5))
                        endif
                        set j=j + 1
                    endif
                    call SaveInteger(udg_ht, task, k, j)
                endif
            endif
        endloop
        call DestroyBoolExpr(f)
