    local integer task
    local real damage
    local real ox= GetUnitX(caster)
    local real oy= GetUnitY(caster)
    local real tx= GetUnitX(target)
    local real ty= GetUnitY(target)
    local real a= bj_RADTODEG * Atan2(ty - oy, tx - ox)
    local effect array e
    //========
