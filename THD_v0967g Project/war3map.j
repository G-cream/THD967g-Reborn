            if ( ( GetUnitLifePercent(GetOrderTargetUnit()) >= 50.00 ) ) then
                call IssuePointOrder(GetTriggerUnit(), "attack", x, y)
                call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "TRIGSTR_6391")
