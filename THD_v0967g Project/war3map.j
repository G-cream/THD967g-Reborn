function Trig_CSS_Random_Ban_Actions takes nothing returns nothing
    local integer c
    local integer chartotal
    local integer m
    local integer n
    local integer w
    local integer i
    local integer index
    set c=0
    set chartotal=R2I(( udg_HeroType[0] + udg_HeroType[100] ) / 2)
    loop
        set c=c + 1
        exitwhen c == chartotal
        set w=GetRandomInt(1, udg_HeroType[0] + udg_HeroType[100])
        set i=Integer(w > udg_HeroType[0])
        set n=i * 100 + 1
        set m=i * 100 + udg_HeroType[i * 100]
        set i=0
        loop
            set index=GetRandomInt(n, m)
            set w=0
            loop
                exitwhen udg_HeroBan[index]
                set w=w + 1
                exitwhen w > 12
            endloop
            set i=i + 1
            exitwhen i >= 50
        endloop
        if w > 11 then
            call CSS_SetCharacterBan(index , true)
        endif
    endloop
endfunction
