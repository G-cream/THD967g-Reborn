            call Trig_AI_Action_Offensive(who , h)
        else
            if not Trig_AI_Action_Maintain(h) then
                call Trig_AI_Action_Defensive(who , h)
            endif
        endif
        if T / 3 * 3 == T then
            call Trig_AI_Action_Custom_Ability(who , h)
        endif
    endif
    call Trig_AI_AddItems(who , h , T)
    //========
    set t=null
    set who=null
    set h=null
endfunction
