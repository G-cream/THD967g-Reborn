function UnitMagicDamageTarget takes unit caster,unit target,real damage returns nothing
    local real outcomedamage= damage
    local real buffingpercentage= 1.00
    if YDWEUnitHasItemOfTypeBJNull(GetPlayerCharacter(GetOwningPlayer(caster)) , 'I064') then //Hakkero
set buffingpercentage=buffingpercentage + 0.30
    endif
    call DebugMsg("MagicDamage:" + R2S(damage))
    set outcomedamage=outcomedamage * buffingpercentage
    call UnitDamageTarget(caster, target, outcomedamage, false, true, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
endfunction
function UnitMagicDamageArea takes unit caster,real x,real y,real range,real damage returns nothing
    local unit v
    local real buffingpercentage= 1.00
    local group g= CreateGroup()
    local boolexpr iff= (udg_FLIFF[GetPlayerId((GetOwningPlayer(caster)))]) // INLINED!!
    call GroupEnumUnitsInRange(g, x, y, range, iff)
    call DebugMsg("MagicDamageArea:" + R2S(damage))
    loop
      set v=FirstOfGroup(g)
      exitwhen v == null
      call GroupRemoveUnit(g, v)
      if IsUnitType(v, UNIT_TYPE_STRUCTURE) == false then
          call UnitMagicDamageTarget(caster , v , damage)
      endif
    endloop
    call DestroyGroup(g)
    set v=null
    set g=null
    set iff=null
endfunction
function UnitHealingTarget takes unit caster,unit target,real damage returns nothing
    local real outcomedamage= damage
    local real buffingpercentage= 1.00
    if YDWEUnitHasItemOfTypeBJNull(GetPlayerCharacter(GetOwningPlayer(caster)) , 'I064') then //Hakkero
set buffingpercentage=buffingpercentage + 0.30
    endif
    set outcomedamage=outcomedamage * buffingpercentage
    call SetUnitState(target, UNIT_STATE_LIFE, GetUnitState(target, UNIT_STATE_LIFE) + outcomedamage)
endfunction
function UnitStunTarget_RemoveBuff takes nothing returns nothing
  local timer t=GetExpiredTimer()
  local integer task=GetHandleId(t)
  local unit caster=LoadUnitHandle(udg_ht, task, 1)
  call UnitRemoveAbility(caster, LoadInteger(udg_ht, task, 2))
  call UnitRemoveAbility(caster, LoadInteger(udg_ht, task, 3))
  call PauseTimer(t)
  call DestroyTimer(t)
  call FlushChildHashtable(udg_ht, task)
  set t=null
  set caster=null
endfunction
function UnitStunTarget takes unit caster,unit target,real time,integer ablty,integer buf returns nothing
    local real outcometime= time
    local integer outcomelevel
    local unit w
    local timer t
    local integer task
    if udg_SK_Parsee01 > 0 and IsUnitEnemy(target, GetOwningPlayer(udg_SK_Parsee)) and IsUnitInRange(target, udg_SK_Parsee, 900.0) then
        set outcometime=outcometime * ( 1 + udg_SK_Parsee01 )
        if outcometime > time + 0.80 then
            set outcometime=time + 0.80
        endif
    endif
    if outcometime > 5.00 then
        set outcometime=5.00
    endif
    set outcomelevel=R2I(outcometime * 20)
    set w=CreateUnit(GetOwningPlayer(target), 'o000', GetUnitX(target), GetUnitY(target), 0)
    call UnitAddAbility(w, 'A0UM')
    call SetUnitAbilityLevel(w, 'A0UM', outcomelevel)
    call IssueTargetOrder(w, "thunderbolt", target)
    if ablty != 0 then
      set t=CreateTimer()
      set task=GetHandleId(t)
      call UnitAddAbility(target, ablty)
      call SaveUnitHandle(udg_ht, task, 1, target)
      call SaveInteger(udg_ht, task, 2, ablty)
      call SaveInteger(udg_ht, task, 3, buf)
      call TimerStart(t, outcometime, false, function UnitStunTarget_RemoveBuff)
    endif
    set w=null
    set t=null
endfunction
function UnitStunArea takes unit caster,real time,real x,real y,real range,integer ablty,integer buf returns nothing
    local real outcometime= time
    local integer outcomelevel
    local unit w
    local unit v
    local timer t
    local integer task
    local group g= CreateGroup()
    local boolexpr iff= (udg_FLIFF[GetPlayerId((GetOwningPlayer(caster)))]) // INLINED!!
    call GroupEnumUnitsInRange(g, x, y, range, iff)
    if outcometime > 5.00 then
        set outcometime=5.00
    endif
    call DebugMsg("StunArea:" + R2S(time) + "sec")
    set outcomelevel=R2I(outcometime * 20)
    loop
      set v=FirstOfGroup(g)
      exitwhen v == null
      call GroupRemoveUnit(g, v)
      call UnitStunTarget(caster , v , time , 0 , 0)
    endloop
    call DestroyGroup(g)
    set v=null
    set g=null
    set w=null
    set t=null
endfunction
